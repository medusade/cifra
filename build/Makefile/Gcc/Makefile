########################################################################
# cifra

# build cifra USRDEFINES
#
build_cifra_USRDEFINES += \
${build_bn_USRDEFINES} \
${build_mp_USRDEFINES} \
${build_mb_USRDEFINES} \
${build_mbuint_USRDEFINES} \
${build_rostra_USRDEFINES} \
${build_nadir_USRDEFINES} \
${build_numera_USRDEFINES} \


# build cifra USRINCLUDES
#
build_cifra_USRINCLUDES += \
${build_numera_USRINCLUDES} \
${build_nadir_USRINCLUDES} \
${build_rostra_USRINCLUDES} \
${build_mbuint_USRINCLUDES} \
${build_mb_USRINCLUDES} \
${build_mp_USRINCLUDES} \
${build_bn_USRINCLUDES} \


# build cifra USRCXXFLAGS
#
build_cifra_USRCXXFLAGS += \
${build_bn_USRCXXFLAGS} \
${build_mp_USRCXXFLAGS} \
${build_mb_USRCXXFLAGS} \
${build_mbuint_USRCXXFLAGS} \
${build_rostra_USRCXXFLAGS} \
${build_nadir_USRCXXFLAGS} \
${build_numera_USRCXXFLAGS} \


# build cifra USRLIBDIRS
#
build_cifra_USRLIBDIRS += \
${build_numera_USRLIBDIRS} \
${build_nadir_USRLIBDIRS} \
${build_rostra_USRLIBDIRS} \
${build_mbuint_USRLIBDIRS} \
${build_mb_USRLIBDIRS} \
${build_mp_USRLIBDIRS} \
${build_bn_USRLIBDIRS} \


# build cifra FRAMEWORKS
#
build_cifra_FRAMEWORKS += \
${build_numera_FRAMEWORKS} \
${build_nadir_FRAMEWORKS} \
${build_rostra_FRAMEWORKS} \
${build_mbuint_FRAMEWORKS} \
${build_mb_FRAMEWORKS} \
${build_mp_FRAMEWORKS} \
${build_bn_FRAMEWORKS} \


# build cifra LIBS
#
build_cifra_LIBS += \
${build_numera_LIBS} \
${build_nadir_LIBS} \
${build_rostra_LIBS} \
${build_mbuint_LIBS} \
${build_mb_LIBS} \
${build_mp_LIBS} \
${build_bn_LIBS} \
${build_cifra_FRAMEWORKS}


