########################################################################
# bn
#
# pkg-config --cflags --libs bn
#

ifndef USE_HOME_BUILD_bn
USE_HOME_BUILD_bn = no
endif

ifeq ($(USE_HOME_BUILD_bn),yes)
#
# home build bn
#
build_bn_USRDEFINES += \

build_bn_USRINCLUDES += \
-I$(HOME)/build/bn/include \

build_bn_USRCXXFLAGS += \

build_bn_USRLIBDIRS += \
-L$(HOME)/build/bn/lib \

build_bn_LIBS += \
-lbn \

build_bn_FRAMEWORKS += \

else
#
# build bn
#
build_bn_USRDEFINES += \

build_bn_USRINCLUDES += \

build_bn_USRCXXFLAGS += \

build_bn_USRLIBDIRS += \

build_bn_LIBS += \

build_bn_FRAMEWORKS += \

endif

